<%= form_for [@product, @booking] do |f| %>
  <%= f.label :start_date %>
  <%= f.date_select :start_date %>

  <%= f.label :end_date %>
  <%= f.date_select :end_date %>

  <%= f.label :product_type %>
  <%= f.select :product_type, ['Gaming PC', 'Consoles', 'Games'] %>

  <%= f.submit 'Book' %>
<% end %>
